{
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "extends": [
        "google"
    ],
    "rules": {
        "require-jsdoc": 0,
        "object-curly-spacing": [
            "error",
            "always"    
        ],
        "semi": 2,
        "no-undef": 2,
        "no-use-before-define": 0
    }
}